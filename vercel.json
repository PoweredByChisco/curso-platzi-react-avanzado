{
    "version" : 2,
    "name": "petgram-fco",
    "builds": [
        {"use": "@vercel/static-build",
        "src": "package.json"}
    ],
    "routes": [
        {
            "src": "(.*).js", "dest": "/$1.js"
        },
        {
            "src": "(.*).json", "dest": "/$1.js"
        },
        {
            "src": "./*", "dest": "/index.html"
        }
    ]
}
